set mainfont {Monaco 8}
set textfont {Monaco 8}
set uifont {Monaco 8 bold}
set tabstop 4
set findmergefiles 0
set maxgraphpct 60
set maxwidth 18
set cmitmode tree
set wrapcomment none
set autoselect 1
set autosellen 40
set showneartags 1
set hideremotes 0
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor grey85
set want_ttk 1
set bgcolor white
set fgcolor black
set colors {green red blue magenta darkgrey brown orange}
set diffcolors {red "#00a000" blue}
set markbgcolor #e0e0ff
set diffcontext 3
set selectbgcolor gray85
set extdifftool meld
set perfile_attrs 0
set geometry(main) 1280x694+0+0
set geometry(state) normal
set geometry(topwidth) 1280
set geometry(topheight) 246
set geometry(pwsash0) "807 1"
set geometry(pwsash1) "1128 1"
set geometry(botwidth) 693
set geometry(botheight) 443
set permviews {{Local {} --branches {}}
{master-fp {} {--first-parent master} {}}
{mmf {} {--all --grep=mmf} {}}
}
